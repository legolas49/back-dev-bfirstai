{
	"name": "back-dev-bfirstai",
	"version": "1.0.0",
	"description": "BFirst AI Backoffice - Environnement de développement",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "node scripts/build-local.js",
		"build:prod": "vite build",
		"preview": "vite preview",
		"prepare": "echo 'Préparation OK - sync ignoré pour compatibilité cross-platform'",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "vitest run",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest --coverage",
		"scan-env": "node scripts/scan-env-simple.js",
		"seed": "tsc src/lib/server/db/seed.ts && node src/lib/server/db/seed.ts",
		"docker:build": "docker build -t ghcr.io/legolas49/back-dev-bfirstai:latest .",
		"docker:run": "docker run -p 3000:3000 ghcr.io/legolas49/back-dev-bfirstai:latest",
		"docker:push": "docker push ghcr.io/legolas49/back-dev-bfirstai:latest"
	},
